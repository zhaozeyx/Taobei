<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

  <TextView
      android:id="@+id/btn_overlook"
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"
      android:layout_gravity="right"
      android:layout_marginRight="@dimen/margin_primary"
      android:layout_marginTop="@dimen/margin_primary"
      android:background="@drawable/bg_btn_yellow_round_corner"
      android:paddingBottom="5dp"
      android:paddingLeft="@dimen/margin_large"
      android:paddingRight="@dimen/margin_large"
      android:paddingTop="5dp"
      android:text="@string/activity_profile_initialize_btn_over_look"
      style="@style/Widget.App.TextView.Medium.White"
      />

  <FrameLayout
      android:id="@+id/content_container"
      android:layout_width="match_parent"
      android:layout_height="match_parent"
      />

</LinearLayout>